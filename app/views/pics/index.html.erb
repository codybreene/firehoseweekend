
<div class="custom-font">


	<h2 class="cool-font-big text-center">Drinking from the Firehose</h2>
	<div class="row">
	<% @pics.each do |pic| %>
	
		<div class="boxy offset1 span3 cool-font-box">
			<h2>
				Hour <%= pic.hour %> 
			</h2>
			<div>
				<%= image_tag pic.image.to_s %>
			</div>

			<p>
				<%= pic.emotion %>
			</p>

			<p>
				I learned  
					<%= pic.learned %>
			</p>
			

		</div>

	<% end %>
</div>


</div>
